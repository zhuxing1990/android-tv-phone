<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--<item android:state_pressed="false" android:drawable="@mipmap/call_audio" >-->

    <!--</item>-->
    <!--<item android:state_pressed="true" android:drawable="@mipmap/call_audio" >-->
    <!--</item>-->
    <item android:state_focused="true">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@color/color_white"></stroke>
            <!-- 使oval_inner透明，裸露出将来设置的背景图片 -->
            <gradient android:centerColor="#0000" />
            <size android:width="50dp"
                  android:height="50dp">
            </size>
        </shape>
    </item>
    <item android:state_focused="false">
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@android:color/transparent"></stroke>
            <!-- 使oval_inner透明，裸露出将来设置的背景图片 -->
            <gradient android:centerColor="#0000" />
            <size android:width="50dp"
                android:height="50dp">
            </size>
        </shape>
    </item>

</selector>